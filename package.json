{
  "name": "fastmile-5g-tools",
  "version": "2.0.0",
  "description": "CLI tool to monitor and manage FastMile 5G gateway - status monitoring, authentication, and interface control",
  "main": "main.js",
  "bin": {
    "fastmile": "./main.js"
  },
  "scripts": {
    "start": "node main.js",
    "status": "node main.js status",
